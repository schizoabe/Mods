                }
            }
        },
        //Unlock Unknowable Dish via the trash
        {
            "Action": "EditData",
            "Target": "Data/TriggerActions",
            "When": {
                "HasMod |contains=spacechase0.SpaceCore": true,
                "alternate_unlock_unknowable": "Off"
            },
            "Entries": {
                "{{ModId}}_garbage_unlock": {
                    "Id": "{{ModId}}_garbage_unlock",
                    "Trigger": "spacechase0.SpaceCore_OnItemUsed",
                    "Condition": "ITEM_ID Input {{ModId}}_unknowable_recipe",
                    "Actions": [
                        "MarkCookingRecipeKnown Current {{ModId}}_unknowable_dish",
                        "spacechase0.SpaceCore_ShowHudMessage \"{{i18n:ui.recipe_unlocked.text}}: {{i18n:item.unknowable_dish.name}}\""
                    ]
                }
            }
        },

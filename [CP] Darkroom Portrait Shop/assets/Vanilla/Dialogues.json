                "RejectItem_(O)AgentLyoko.PortraitShop_PortraitTicket": "{{i18n:PortraitShop.GlobalReject}}",
			},
			"When": {
				"HasMod |contains=spacechase0.SpaceCore": false, //If true, it should be using the SpaceCore block instead.
			},
        },
		
		{
            "LogName": "Reject Ticket - Sandy Special",
			"Action": "EditData",
            "Target": "Characters/Dialogue/Sandy",
            "Entries": {
                "RejectItem_(O)AgentLyoko.PortraitShop_PortraitTicket": "{{i18n:PortraitShop.GlobalReject}}",
			},
			"When": {
				"HasMod |contains=spacechase0.SpaceCore": false, //If true, it should be using the SpaceCore block instead. Sandy gets her own in case she becomes romanceable.
			},
        },
		
		{
            "LogName": "Reject Ticket - Wizard Withdrawn Without Magnus Madness",
			"Action": "EditData",
            "Target": "Characters/Dialogue/Wizard",
            "Entries": {
                "RejectItem_(O)AgentLyoko.PortraitShop_PortraitTicket": "{{i18n:PortraitShop.GlobalReject}}",
			},
			"When": {
				"HasMod |contains=spacechase0.SpaceCore": false, //If true, it should be using the SpaceCore block instead.
